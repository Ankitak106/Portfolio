<%- include("includes/header.ejs") %>
<div class="card">
<form class="for" method="POST" action="/register">
    <div class="dv">
        <h2>CONNECT WITH US</h2>
    <label>Username</label>
    <input class="inp" placeholder="Enter your name" name="user" type="text" />
    </div><br/>
    <div class="dv">
    <label>User Email</label>
    <input class="inp" placeholder="Enter your Email" name="email" type="text" />
    </div><br/>
    <div class="dv">
    <label>Message</label>
    <textarea placeholder="Enter your message!" name="message" type="text"></textarea>
    </div><br/>
    <button>Submit</button>
</form>
</div>
<%- include("includes/footer.ejs") %> 